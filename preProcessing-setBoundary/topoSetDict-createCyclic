actions
(
    {
        name    cyclicFacesFaceSet;
        type    faceSet;
        action  new;
        source  cylinderToFace;
        p1            (0 0 -3.001);
        p2            (0 0 -2.698);
        radius        0.678;
        innerRadius   0.676;
    }

    {
        name    cyclicFacesSlaveCells;
        type    cellSet;
        action  new;
        source  cylinderToCell;
        p1      (0 0 -3.001);    // 圆柱轴线起点（底部中心）
        p2      (0 0 -2.698);     // 圆柱轴线终点（顶部中心）
        radius  0.677;               // 圆柱半径
    }

    {
        name    cyclicFaces;
        type    faceZoneSet;
        action  new;
        source  setsToFaceZone;
        faceSet cyclicFacesFaceSet;
        cellSet cyclicFacesSlaveCells;
    }
);

